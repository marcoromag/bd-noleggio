RewriteEngine On
RewriteBase /
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^noleggio/api/(.+)$ /noleggio/api/index.php [QSA,L]

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^noleggio/webapp/(.+)$ /noleggio/webapp/index.html [QSA,L]

php_flag display_startup_errors on
php_flag display_errors on